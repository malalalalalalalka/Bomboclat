	local function SVUYV_fake_script() -- Frame_3.LocalScript 
		local script = Instance.new('LocalScript', Frame_3)
	
	
	
		while true do 
			wait(1)
			local Players = game:GetService("Players")
			local RunService = game:GetService("RunService")
			local TweenService = game:GetService("TweenService")
			local Workspace = game:GetService("Workspace")
			local ReplicatedStorage = game:GetService("ReplicatedStorage")
	
			local player = Players.LocalPlayer
			local character = player.Character or player.CharacterAdded:Wait()
			local humanoid = character:WaitForChild("Humanoid")
			local rootPart = character:WaitForChild("HumanoidRootPart")
	
			local targetPlayer
			local headSit
			local sitConnection
			local messageCounter = 0
			local targetPositionBackup
			local originalPosition -- Variable to store the original position
	
			local textbox = script.Parent:WaitForChild("tb1") -- Replace with the path to your tb1
			local enterButton = script.Parent:WaitForChild("ad") -- Replace with the path to your Enter Button
	
			local isChatting = false
			local isSpinning = false
			local isSitting = false
			local isVerticalMovementEnabled = true -- Flag to control vertical movement
	
		
			local function findPlayerByPartialName(partialName)
				local players = Players:GetPlayers()
				for _, otherPlayer in ipairs(players) do
					if otherPlayer.Name:lower():sub(1, #partialName) == partialName:lower() then
						return otherPlayer
					end
				end
				return nil
			end
	
		
			local function getSafePosition(position)
				local ray = Ray.new(position + Vector3.new(0, 10, 0), Vector3.new(0, -20, 0))
				local hitPart, hitPosition = Workspace:FindPartOnRay(ray)
				if hitPart then
					return hitPosition + Vector3.new(0, 1, 0)
				else
					return position
				end
			end
	
			-- Function to dive onto the target player's HumanoidRootPart
			local function diveOntoPlayer(target)
				if not target or not target.Character or not target.Character:FindFirstChild("HumanoidRootPart") then
					return
				end
	
				targetPositionBackup = target.Character.HumanoidRootPart.Position
				originalPosition = rootPart.Position -- Store original position before diving
				local targetPosition = getSafePosition(targetPositionBackup)
	
				local diveTween = TweenService:Create(
					rootPart,
					TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut),
					{CFrame = CFrame.new(targetPosition + Vector3.new(0, 5, 0))}
				)
	
				diveTween:Play()
				diveTween.Completed:Connect(function()
					rootPart.CFrame = target.Character.HumanoidRootPart.CFrame * CFrame.new(0, 1.6, 0.4)
					humanoid.Sit = true
					isSitting = true
					isVerticalMovementEnabled = true
					tweenUpAndDown()
					headSit = RunService.Heartbeat:Connect(function()
						if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") and humanoid.Sit then
							rootPart.CFrame = target.Character.HumanoidRootPart.CFrame * CFrame.new(0, 1.6, 0.4)
						else
							headSit:Disconnect()
						end
					end)
				end)
			end
	
			-- Function to spin and look at the target player for a longer duration
			local function spinAndLook(target, duration)
				if isSpinning then return end
				isSpinning = true
	
				local startTime = tick()
				local spinTween = TweenService:Create(
					rootPart,
					TweenInfo.new(duration, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut),
					{CFrame = CFrame.new(target.Character.HumanoidRootPart.Position)}
				)
	
				spinTween:Play()
				spinTween.Completed:Connect(function()
					isSpinning = false
					chatFunnyMessage(target.Name) -- Ensure chat happens after spinning completes
				end)
			end
	
	
			local function sitOnTarget(target)
				if sitConnection then sitConnection:Disconnect() end
				isSitting = true
				isVerticalMovementEnabled = true
				sitConnection = RunService.Heartbeat:Connect(function()
					if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
						local targetCFrame = target.Character.HumanoidRootPart.CFrame
	
						if isVerticalMovementEnabled then
							local amplitude = 2.2
							local frequency = (messageCounter > 4) and 6 or 16
							local heightOffset = math.sin(tick() * frequency) * amplitude
							local leanForwardCFrame = targetCFrame * CFrame.Angles(math.rad(-90), 0, 0)
							rootPart.CFrame = leanForwardCFrame * CFrame.new(0, 1.6 + heightOffset, -1)
						else
							rootPart.CFrame = targetCFrame * CFrame.new(0, 1.6, -1)
						end
					else
						wait(1)
						if isSitting then
							sitConnection:Disconnect()
							isSitting = false
						end
					end
				end)
			end
	
			-- Function to play a specific animation
			local function playAnimation()
				local animationId = "rbxassetid://188632011"
				local animation = Instance.new("Animation")
				animation.AnimationId = animationId
	
				local animationTrack = humanoid:LoadAnimation(animation)
				if animationTrack then
					print("Animation loaded successfully.")
					animationTrack:AdjustSpeed(5)
					animationTrack:Play()
					wait(0.5)
				else
					print("Error: Could not load animation with ID: " .. animationId)
				end
			end
	
			-- Function to chat funny messages
			local function chatFunnyMessage(targetName)
				if isChatting then return end
				isChatting = true
	
				local messages = {
					"Watch out, " .. targetName .. "! 😆",
					"You're DONE, " .. targetName .. "! 😎",
					"Get ready, " .. targetName .. "! 😂",
					"Incoming! " .. targetName .. "!!! 🤪",
					"Feeling the rizz, " .. targetName .. "! 😜"
				}
	
				for _, message in ipairs(messages) do
					ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(message, "All")
					wait(1)
				end
	
				isChatting = false
				-- Call stopSitting after chatting is done
				stopSitting()
	
				-- Wait for 0.5 seconds then set the humanoid health to 0
				wait(0.5)
				humanoid.Health = 0
			end
	
			-- Autocomplete functionality
			local function updateTextboxSuggestions()
				local partialName = textbox.Text
				if #partialName > 3 then
					local player = findPlayerByPartialName(partialName)
					if player then
						textbox.Text = player.Name
					end
				end
			end
	
			-- Event handler for text input
			textbox:GetPropertyChangedSignal("Text"):Connect(updateTextboxSuggestions)
	
			-- Event handler for pressing the Enter button
			enterButton.MouseButton1Click:Connect(function()
				local partialName = textbox.Text
				if partialName and partialName ~= "" then
					targetPlayer = findPlayerByPartialName(partialName)
	
					if targetPlayer then
						playAnimation()
						spinAndLook(targetPlayer, 3)
						diveOntoPlayer(targetPlayer)
						sitOnTarget(targetPlayer)
					else
						ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("No player found with the name: " .. partialName, "All")
					end
				end
			end)
	
			-- Function to stop sitting and tween back to the original position
			local function stopSitting()
				if isSitting then
					isSitting = false
					isVerticalMovementEnabled = false -- Disable vertical movement
					if sitConnection then
						sitConnection:Disconnect()
						sitConnection = nil
					end
					humanoid.Sit = false
	
					-- Tween back to the original position
					local returnTween = TweenService:Create(
						rootPart,
						TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut),
						{CFrame = CFrame.new(originalPosition)}
					)
	
					returnTween:Play()
					returnTween.Completed:Connect(function()
						-- Optional: Reset orientation if needed
						rootPart.CFrame = CFrame.new(originalPosition)
					end)
				end
			end
	
		end
	
	end
	coroutine.wrap(SVUYV_fake_script)()
	local function DSKON_fake_script() -- re.LocalScript 
