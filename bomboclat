	-- Scripts
	local Players = game:GetService("Players")
    local function scanPlayers(query)
        local players = Players:GetPlayers()
        local matches = {}
    
        for _, player in ipairs(players) do
            local usernameMatch = string.sub(player.Name:lower(), 1, #query:lower()) == query:lower()
            local displayNameMatch = string.sub(player.DisplayName:lower(), 1, #query:lower()) == query:lower()
    
            if usernameMatch or displayNameMatch then
                table.insert(matches, player.Name)
            end
        end
    
        return matches
    end
    
    local function autoComplete()
        local inputText = tb1.Text
        local matches = scanPlayers(inputText)
    
        if #matches == 1 then
            tb1.Text = matches[1]
            tb1.SelectionStart = #tb1.Text -- Keep the cursor at the end of the text
            tb1.TextEditable = true
        else
            tb1.TextEditable = true
        end
    end
    
    tb1:GetPropertyChangedSignal("Text"):Connect(function()
        autoComplete()
    end)
    
    tb1.FocusLost:Connect(function(enterPressed)
        if enterPressed then
            local targetUsername = tb1.Text
            if Players:FindFirstChild(targetUsername) then
                print("Target player found: " .. targetUsername)
            else
                print("No matching player found.")
            end
        end
    end)

	local function ORDEQXB_fake_script() -- TextButton.LocalScript 
		local script = Instance.new('LocalScript', TextButton)
	
		script.Parent.MouseButton1Down:Connect(function()
			local Players = game:GetService("Players")
			local RunService = game:GetService("RunService")
			local TweenService = game:GetService("TweenService")
			local Workspace = game:GetService("Workspace")
	
			local player = Players.LocalPlayer
			local character = player.Character or player.CharacterAdded:Wait()
			local humanoid = character:WaitForChild("Humanoid")
			local rootPart = character:WaitForChild("HumanoidRootPart")
	
			local targetPlayer
			local headSit
			local following
			local sitConnection
			local messageCounter = 0
			local targetPositionBackup -- Variable to store the target position
	
			-- Function to get a random player within 15 studs of the local player
			local function getRandomPlayerNearby()
				local nearbyPlayers = {}
				for _, otherPlayer in pairs(Players:GetPlayers()) do
					if otherPlayer ~= player and otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
						local distance = (otherPlayer.Character.HumanoidRootPart.Position - rootPart.Position).Magnitude
						if distance <= 15 then
							table.insert(nearbyPlayers, otherPlayer)
						end
					end
				end
				if #nearbyPlayers > 0 then
					return nearbyPlayers[math.random(1, #nearbyPlayers)]
				end
			end
	
			-- Function to ensure the position is on a solid part of the map
			local function getSafePosition(position)
				local ray = Ray.new(position + Vector3.new(0, 10, 0), Vector3.new(0, -20, 0))
				local hitPart, hitPosition = Workspace:FindPartOnRay(ray)
				if hitPart then
					return hitPosition + Vector3.new(0, 1, 0) -- Slight adjustment to ensure the position is above the hit part
				else
					return position
				end
			end
	
			-- Function to dive onto the target player's HumanoidRootPart
			local function diveOntoPlayer(target)
				if not target or not target.Character or not target.Character:FindFirstChild("HumanoidRootPart") then
					return
				end
	
				targetPositionBackup = target.Character.HumanoidRootPart.Position -- Store target position
				local targetPosition = getSafePosition(targetPositionBackup) -- Get safe position
	
				local diveTween = TweenService:Create(
					rootPart,
					TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut),
					{CFrame = CFrame.new(targetPosition + Vector3.new(0, 5, 0))}
				)
	
				diveTween:Play()
				diveTween.Completed:Connect(function()
					-- Ensure the character lands correctly on the HumanoidRootPart
					rootPart.CFrame = target.Character.HumanoidRootPart.CFrame * CFrame.new(0, 1.6, 0.4)
					humanoid.Sit = true
					tweenUpAndDown()
					headSit = RunService.Heartbeat:Connect(function()
						if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") and humanoid.Sit then
							rootPart.CFrame = target.Character.HumanoidRootPart.CFrame * CFrame.new(0, 1.6, 0.4)
						else
							headSit:Disconnect()
						end
					end)
				end)
			end
	
			-- Function to spin and look at the target player for a longer duration
			local function spinAndLook(target, duration)
				local startTime = tick()
				following = RunService.RenderStepped:Connect(function()
					if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart")    then
						local targetCFrame = target.Character.HumanoidRootPart.CFrame
						local direction = (targetCFrame.Position - rootPart.Position).unit
						local lookAtCFrame = CFrame.new(rootPart.Position, targetCFrame.Position)
						rootPart.CFrame = lookAtCFrame
	
						-- Increase spin speed by modifying the rotation angle per second
						local spinSpeed = 3000 -- Degrees per second
						local elapsed = tick() - startTime
						local angle = math.rad(spinSpeed * elapsed)
	
						rootPart.CFrame = lookAtCFrame * CFrame.Angles(0, angle, 0)
	
						-- Stop spinning when the duration is over
						if elapsed >= duration then
							following:Disconnect()
						end
					end
				end)
			end
	
			-- Function to rapidly adjust CFrame while sitting on the target player's HumanoidRootPart
			local function sitOnTarget(target)
				if sitConnection then sitConnection:Disconnect() end
				sitConnection = RunService.Heartbeat:Connect(function()
					if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart")  then
						local targetCFrame = target.Character.HumanoidRootPart.CFrame
	
						-- Increase the multiplier for the height change and speed of oscillation
						local amplitude = 2.2 -- Higher amplitude for more vertical movement
						local frequency = (messageCounter > 4) and 6 or 16 -- Adjust frequency after 3rd message
						local heightOffset = math.sin(tick() * frequency) * amplitude
	
						-- Lean forward by 180 degrees while oscillating
						local leanForwardCFrame = targetCFrame * CFrame.Angles(math.rad(-90), 0, 0) 
						rootPart.CFrame = leanForwardCFrame * CFrame.new(0, 1.6 + heightOffset, -1)
					else
						wait(1)
						sitConnection:Disconnect()
					end
				end)
	
				-- Function to send specific chat messages with delays
				local function sendChatMessages()
					local messages = {
					
						{text = "wһуаrеуоujасkіngоffiгl", delay = 2},
						{text = "іmgоnnаgіvеуоuаіdѕ", delay = 2.5},
						{text = "your quееr for me🥵", delay = 2},
						
                        {text = "yeah sigmarape and make me feel good", delay = 2},
                        {text = "OH MY GOD I JUST GOONED EVERYWHERE", delay = 2},
	
	
					}
	
					for _, message in ipairs(messages) do
						game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(message.text, "All")
						messageCounter = messageCounter + 1 -- Increment message counter
						wait(message.delay) -- Delay between messages
					end
	
					-- Stop teleporting after messages are sent
					humanoid.Sit = false
					rootPart.CFrame = target.Character.HumanoidRootPart.CFrame * CFrame.new(0, 1.6, 0.4)
					sitConnection:Disconnect() -- Ensure to disconnect after sending all messages
	
					-- Move the character back to the target's position after unsitting
					if targetPositionBackup then
						local safePosition = getSafePosition(targetPositionBackup)
						local returnTween = TweenService:Create(
							rootPart,
							TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut),
							{CFrame = CFrame.new(safePosition + Vector3.new(0, 5, 0))}
						)
						returnTween:Play()
					end
				end
	
				-- Start sending specific chat messages
				sendChatMessages()
			end
	
			-- Function to play a specific animation
			local function playAnimation()
				local animationId = "rbxassetid://188632011"
				local animation = Instance.new("Animation")
				animation.AnimationId = animationId
				local animationTrack = humanoid:LoadAnimation(animation)
	
				-- Set the playback speed before playing using AdjustSpeed
				animationTrack:AdjustSpeed(5)
	
				-- Play the animation
				animationTrack:Play()
	
				-- Wait for a while before stopping
				wait(0.5)
			end
	
			-- Function to chat funny messages
			local function chatFunnyMessage(targetName)
				local messages = {
					"Watch out, " .. targetName .. "! 😆",
					"You're DONE, " .. targetName .. "! 😎",
					"Get ready, " .. targetName .. "! 😂",
					"Incoming! " .. targetName .. " 🚀",
					"Brace yourself, " .. targetName .. "! 🌟",
				}
				local randomMessage = messages[math.random(1, #messages)]
				game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(randomMessage, "All")
				wait(3)
			end
	
			-- Function to handle target player leaving the game
			
	
			-- Main execution
			targetPlayer = getRandomPlayerNearby()
			if targetPlayer then
				playAnimation()  -- Use the updated function
				local spinDuration = 5 -- Duration to spin and look at the target player
				spinAndLook(targetPlayer, spinDuration)
				wait(spinDuration)
				chatFunnyMessage(targetPlayer.Name)
				if following then following:Disconnect() end
				wait(1)
				diveOntoPlayer(targetPlayer)
				wait(2)
				sitOnTarget(targetPlayer)
	
				-- Connect to target player's character removal
				targetPlayer.CharacterRemoving:Connect(onTargetPlayerRemoved)
			else
				game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("No targets within range! 💀", "All")
				humanoid.Health = 0
			end
	
		end)
	end
	coroutine.wrap(ORDEQXB_fake_script)()
	local function TWIMIHT_fake_script() -- TextButton_2.LocalScript 
		local script = Instance.new('LocalScript', TextButton_2)
	
		script.Parent.MouseButton1Down:Connect(function()
			-- Services
			local TweenService = game:GetService("TweenService")
			local StarterGui = game:GetService("StarterGui")
	
			-- Link to copy
			local linkToCopy = "https://discord.gg/C7GPPaADFA"
	
			-- Function to copy link to clipboard (Note: This requires a specific permission and may not work in all environments)
			local function copyToClipboard(link)
				if setclipboard then
					setclipboard(link)
				else
					warn("Copy to clipboard is not supported on this platform.")
				end
			end
	
			-- Function to create and display a tweening notification
			local function createNotification(message)
				-- Create a ScreenGui to hold the notification
				local screenGui = Instance.new("ScreenGui")
				screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	
				-- Create the notification frame
				local notificationFrame = Instance.new("Frame")
				notificationFrame.Size = UDim2.new(0, 300, 0, 50)
				notificationFrame.Position = UDim2.new(0.5, -150, 1, 0) -- Start below the screen
				notificationFrame.AnchorPoint = Vector2.new(0.5, 1)
				notificationFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
				notificationFrame.BorderSizePixel = 0
				notificationFrame.Parent = screenGui
	
				-- Create a label for the message
				local messageLabel = Instance.new("TextLabel")
				messageLabel.Size = UDim2.new(1, 0, 1, 0)
				messageLabel.Position = UDim2.new(0, 0, 0, 0)
				messageLabel.BackgroundTransparency = 1
				messageLabel.Text = message
				messageLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
				messageLabel.Font = Enum.Font.Gotham
				messageLabel.TextScaled = true
				messageLabel.Parent = notificationFrame
	
				-- Tween the notification into view
				local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
				local tweenPosition = TweenService:Create(notificationFrame, tweenInfo, {Position = UDim2.new(0.5, -150, 0.9, 0)})
				tweenPosition:Play()
	
				-- Wait a bit and then tween it out of view
				wait(2)
				local tweenOut = TweenService:Create(notificationFrame, tweenInfo, {Position = UDim2.new(0.5, -150, 1, 0)})
				tweenOut:Play()
	
				-- Destroy the ScreenGui after the tween
				tweenOut.Completed:Connect(function()
					screenGui:Destroy()
				end)
			end
	
			-- Function to handle the button click
			local function onButtonClick()
				-- Copy the link to clipboard
				copyToClipboard(linkToCopy)
	
				-- Show the notification
				createNotification("Link copied to clipboard!")
			end
	
			-- Connect the button click event
			local button = script.Parent -- Assuming this script is inside the button
			button.MouseButton1Click:Connect(onButtonClick)
	
		end)
	end
	coroutine.wrap(TWIMIHT_fake_script)()
	local function WVDCLZ_fake_script() -- ImageButton.LocalScript 
		local script = Instance.new('LocalScript', ImageButton)
	
		script.Parent.MouseButton1Down:Connect(function()
			-- Services
			local TweenService = game:GetService("TweenService")
			local StarterGui = game:GetService("StarterGui")
	
			-- Link to copy
			local linkToCopy = "https://discord.gg/C7GPPaADFA"
	
			-- Function to copy link to clipboard (Note: This requires a specific permission and may not work in all environments)
			local function copyToClipboard(link)
				if setclipboard then
					setclipboard(link)
				else
					warn("Copy to clipboard is not supported on this platform.")
				end
			end
	
			-- Function to create and display a tweening notification
			local function createNotification(message)
				-- Create a ScreenGui to hold the notification
				local screenGui = Instance.new("ScreenGui")
				screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	
				-- Create the notification frame
				local notificationFrame = Instance.new("Frame")
				notificationFrame.Size = UDim2.new(0, 300, 0, 50)
				notificationFrame.Position = UDim2.new(0.5, -150, 1, 0) -- Start below the screen
				notificationFrame.AnchorPoint = Vector2.new(0.5, 1)
				notificationFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
				notificationFrame.BorderSizePixel = 0
				notificationFrame.Parent = screenGui
	
				-- Create a label for the message
				local messageLabel = Instance.new("TextLabel")
				messageLabel.Size = UDim2.new(1, 0, 1, 0)
				messageLabel.Position = UDim2.new(0, 0, 0, 0)
				messageLabel.BackgroundTransparency = 1
				messageLabel.Text = message
				messageLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
				messageLabel.Font = Enum.Font.Gotham
				messageLabel.TextScaled = true
				messageLabel.Parent = notificationFrame
	
				-- Tween the notification into view
				local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
				local tweenPosition = TweenService:Create(notificationFrame, tweenInfo, {Position = UDim2.new(0.5, -150, 0.9, 0)})
				tweenPosition:Play()
	
				-- Wait a bit and then tween it out of view
				wait(2)
				local tweenOut = TweenService:Create(notificationFrame, tweenInfo, {Position = UDim2.new(0.5, -150, 1, 0)})
				tweenOut:Play()
	
				-- Destroy the ScreenGui after the tween
				tweenOut.Completed:Connect(function()
					screenGui:Destroy()
				end)
			end
	
			-- Function to handle the button click
			local function onButtonClick()
				-- Copy the link to clipboard
				copyToClipboard(linkToCopy)
	
				-- Show the notification
				createNotification("Link copied to clipboard!")
			end
	
			-- Connect the button click event
			local button = script.Parent -- Assuming this script is inside the button
			button.MouseButton1Click:Connect(onButtonClick)
	
		end)
	end
	coroutine.wrap(WVDCLZ_fake_script)()
