function loading()
    OrionLib:MakeNotification({
        Name = "Successfully executed Menace Hub!",
        Content = "Loading...",
        Image = "rbxassetid://119780129386155",
        Time = 5
    })
end   
function alrexe()
    OrionLib:MakeNotification({
        Name = "Error.",
        Content = "Menace Hub has already been executed.",
        Image = "rbxassetid://127494006155047",
        Time = 5
    })
end
function loaded()
    OrionLib:MakeNotification({
        Name = "Successfully executed Menace Hub!",
        Content = "Loaded!",
        Image = "rbxassetid://119780129386155",
        Time = 5
    })
end

